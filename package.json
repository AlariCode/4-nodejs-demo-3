{
	"name": "dashboard-api",
	"version": "1.0.0",
	"description": "API для учебного проекта 4-NodeJS",
	"main": "index.js",
	"type": "commonjs",
	"scripts": {
		"start": "node ./dist/main.js",
		"dev": "nodemon",
		"dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
		"lint": "eslint ./src/**",
		"test": "jest",
		"test:e2e": "jest --config jest.e2e.config.ts --coverage",
		"generate": "prisma generate",
		"lint:fix": "eslint ./src/** --fix",
		"build": "tsc"
	},
	"author": "Anton Larichev",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "^3.3.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"class-transformer": "^0.4.0",
		"class-validator": "^0.13.1",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"inversify": "^6.0.1",
		"jsonwebtoken": "^8.5.1",
		"reflect-metadata": "^0.1.13",
		"tslog": "^3.2.2"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/express": "^4.17.13",
		"@types/jest": "^27.0.2",
		"@types/jsonwebtoken": "^8.5.5",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^27.3.1",
		"nodemon": "^2.0.13",
		"prettier": "^2.4.1",
		"prisma": "^3.3.0",
		"supertest": "^6.1.6",
		"ts-jest": "^27.0.7",
		"ts-node": "^10.3.0",
		"typescript": "^4.4.4"
	}
}